<%- include("partials/header") -%>
<h1>update</h1>
<form action="update" method="post">
  <div class="form-group">
    <label for="as">title </label>
    <input
      id="as"
      type="text"
      class="form-control"
      name="composeTitle"
      value="<%= postTitle %>"
    />
  </div>
  <div class="form-group">
    <label for="ps">post </label>
    <textarea
      id="ps"
      name="composePost"
      cols="30"
      rows="10"
      class="form-control"
    >
    <%= postPost %>
</textarea
    >
  </div>

  <button
    class="btn btn-primary"
    type="submit"
    name="update"
    value="<%= postId %>"
  >
    update
  </button>
</form>
<%- include("partials/footer") -%>
